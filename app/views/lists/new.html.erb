<h2> New List </h2>

<%= form_for @list do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :list_type %>
<%= f.text_field :list_type %>
<%= f.label :category %>
<%= f.text_field :category %>
<%= f.label :list_time %>
<%= f.text_field :list_time %>
<%= f.label :deadline %>
<%= f.text_field :deadline %>
<%= f.label :recurring %>
<%= f.text_field :recurring %>
<%= f.submit %>
<% end %>
